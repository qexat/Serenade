#include "config.h"

#ifdef CFLAGS
#ifdef __NetBSD__
-I/usr/pkg/include
#endif
#endif

#ifdef LIBS
#ifdef HAS_FFI_SUPPORT
#ifdef __NetBSD__
-Wl,-R/usr/pkg/lib -L/usr/pkg/lib
#endif
-lffi
#endif
#ifdef __linux__
#ifndef __MINGW32__
-ldl
#endif
#endif
#endif

#ifdef OBJS
#ifdef HAS_FFI_SUPPORT
ffi_binding.o
#endif
#endif
